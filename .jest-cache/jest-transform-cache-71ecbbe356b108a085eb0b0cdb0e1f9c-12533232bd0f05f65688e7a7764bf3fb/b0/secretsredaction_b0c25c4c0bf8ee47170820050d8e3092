  }

  /**
   * Add custom pattern at runtime
   */
  addCustomPattern(pattern) {
    /* istanbul ignore next */
    cov_259dpoh0rz().f[20]++;
    const compiledPattern =
    /* istanbul ignore next */
    (cov_259dpoh0rz().s[88]++, {
      ...pattern,
      compiledRegex: new RegExp(pattern.regex, 'gi')
    });
    /* istanbul ignore next */
    cov_259dpoh0rz().s[89]++;
    this.patterns.push(compiledPattern);
  }

  /**
   * Redact secrets from value (replace with [REDACTED])
   */
  redactSecrets(value) {
    /* istanbul ignore next */
    cov_259dpoh0rz().f[21]++;
    let redactedValue =
    /* istanbul ignore next */
    (cov_259dpoh0rz().s[90]++, value);
    /* istanbul ignore next */
    cov_259dpoh0rz().s[91]++;
    if (typeof value === 'object') {
      /* istanbul ignore next */
      cov_259dpoh0rz().b[21][0]++;
      cov_259dpoh0rz().s[92]++;
      redactedValue = JSON.stringify(value, null, 2);
    } else {
      /* istanbul ignore next */
      cov_259dpoh0rz().b[21][1]++;
      cov_259dpoh0rz().s[93]++;
      if (typeof value !== 'string') {
        /* istanbul ignore next */
        cov_259dpoh0rz().b[22][0]++;
        cov_259dpoh0rz().s[94]++;
        redactedValue = String(value);
      } else
      /* istanbul ignore next */
      {
        cov_259dpoh0rz().b[22][1]++;
      }
    }
    /* istanbul ignore next */
    cov_259dpoh0rz().s[95]++;
    for (const pattern of this.patterns) {
      /* istanbul ignore next */
      cov_259dpoh0rz().s[96]++;
      redactedValue = redactedValue.replace(pattern.compiledRegex, '[REDACTED]');
    }
    /* istanbul ignore next */
    cov_259dpoh0rz().s[97]++;
    return redactedValue;
  }
}

// Export singleton instance
let instance =
/* istanbul ignore next */
(cov_259dpoh0rz().s[98]++, null);
function getRedactorInstance(patternsPath =
/* istanbul ignore next */
(cov_259dpoh0rz().b[23][0]++, null)) {
  /* istanbul ignore next */
  cov_259dpoh0rz().f[22]++;
  cov_259dpoh0rz().s[99]++;
  if (!instance) {
    /* istanbul ignore next */
    cov_259dpoh0rz().b[24][0]++;
    cov_259dpoh0rz().s[100]++;
    instance = new SecretsRedactor(patternsPath);
  } else
  /* istanbul ignore next */
  {
    cov_259dpoh0rz().b[24][1]++;
  }
  cov_259dpoh0rz().s[101]++;
  return instance;
}
/* istanbul ignore next */
cov_259dpoh0rz().s[102]++;
module.exports = {
  SecretsRedactor,
  getRedactorInstance,
  validateNoSecrets: (key, value) => {
    /* istanbul ignore next */
    cov_259dpoh0rz().f[23]++;
    cov_259dpoh0rz().s[103]++;
    return getRedactorInstance().validateNoSecrets(key, value);
  },
  scanForSecrets: value => {
    /* istanbul ignore next */
    cov_259dpoh0rz().f[24]++;
    cov_259dpoh0rz().s[104]++;
    return getRedactorInstance().scanForSecrets(value);
  },
  redactSecrets: value => {
    /* istanbul ignore next */
    cov_259dpoh0rz().f[25]++;
    cov_259dpoh0rz().s[105]++;
    return getRedactorInstance().redactSecrets(value);
  }
};
