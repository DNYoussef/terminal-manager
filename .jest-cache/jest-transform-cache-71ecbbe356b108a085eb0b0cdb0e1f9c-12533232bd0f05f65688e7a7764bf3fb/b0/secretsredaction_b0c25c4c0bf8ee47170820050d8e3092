    (cov_259dpoh0rz().b[1][1]++, path.join(__dirname, 'secrets-patterns.json'));
    /* istanbul ignore next */
    cov_259dpoh0rz().s[3]++;
    this.patterns = [];
    /* istanbul ignore next */
    cov_259dpoh0rz().s[4]++;
    this.whitelistedPatterns = [];
    /* istanbul ignore next */
    cov_259dpoh0rz().s[5]++;
    this.config = {};
    /* istanbul ignore next */
    cov_259dpoh0rz().s[6]++;
    this.violationLog = [];
    /* istanbul ignore next */
    cov_259dpoh0rz().s[7]++;
    this.loadPatterns();
  }

  /**
   * Load secret patterns from configuration file
   */
  loadPatterns() {
    /* istanbul ignore next */
    cov_259dpoh0rz().f[1]++;
    cov_259dpoh0rz().s[8]++;
    try {
      const data =
      /* istanbul ignore next */
      (cov_259dpoh0rz().s[9]++, fs.readFileSync(this.patternsPath, 'utf8'));
      const config =
      /* istanbul ignore next */
      (cov_259dpoh0rz().s[10]++, JSON.parse(data));

      // Compile regex patterns for performance
      /* istanbul ignore next */
      cov_259dpoh0rz().s[11]++;
      this.patterns = config.patterns.map(pattern => {
        /* istanbul ignore next */
        cov_259dpoh0rz().f[2]++;
        cov_259dpoh0rz().s[12]++;
        return {
          ...pattern,
          compiledRegex: new RegExp(pattern.regex, 'gi')
        };
      });

      // Compile whitelist patterns
      /* istanbul ignore next */
      cov_259dpoh0rz().s[13]++;
      this.whitelistedPatterns = (
      /* istanbul ignore next */
      (cov_259dpoh0rz().b[2][0]++, config.whitelisted_patterns) ||
      /* istanbul ignore next */
      (cov_259dpoh0rz().b[2][1]++, [])).map(pattern => {
        /* istanbul ignore next */
        cov_259dpoh0rz().f[3]++;
        cov_259dpoh0rz().s[14]++;
        return new RegExp(pattern, 'i');
      });
      /* istanbul ignore next */
      cov_259dpoh0rz().s[15]++;
      this.config =
      /* istanbul ignore next */
      (cov_259dpoh0rz().b[3][0]++, config.config) ||
      /* istanbul ignore next */
      (cov_259dpoh0rz().b[3][1]++, {});
      /* istanbul ignore next */
      cov_259dpoh0rz().s[16]++;
      return true;
    } catch (error) {
      /* istanbul ignore next */
      cov_259dpoh0rz().s[17]++;
      console.error('Failed to load secrets patterns:', error.message);
      /* istanbul ignore next */
      cov_259dpoh0rz().s[18]++;
      throw new Error(`Secrets redaction initialization failed: ${error.message}`);
    }
  }

  /**
   * Reload patterns from file (useful for dynamic updates)
