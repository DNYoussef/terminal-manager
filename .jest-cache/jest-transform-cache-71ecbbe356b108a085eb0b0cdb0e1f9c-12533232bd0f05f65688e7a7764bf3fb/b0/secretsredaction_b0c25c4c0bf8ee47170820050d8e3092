    /* istanbul ignore next */
    cov_259dpoh0rz().s[21]++;
    this.loadPatterns();
  }

  /**
   * Check if value is whitelisted (test data, examples, etc.)
   */
  isWhitelisted(value) {
    /* istanbul ignore next */
    cov_259dpoh0rz().f[5]++;
    cov_259dpoh0rz().s[22]++;
    if (typeof value !== 'string') {
      /* istanbul ignore next */
      cov_259dpoh0rz().b[4][0]++;
      cov_259dpoh0rz().s[23]++;
      value = JSON.stringify(value);
    } else
    /* istanbul ignore next */
    {
      cov_259dpoh0rz().b[4][1]++;
    }
    cov_259dpoh0rz().s[24]++;
    return this.whitelistedPatterns.some(pattern => {
      /* istanbul ignore next */
      cov_259dpoh0rz().f[6]++;
      cov_259dpoh0rz().s[25]++;
      return pattern.test(value);
    });
  }

  /**
   * Scan value for secrets
   * @param {string|object} value - Value to scan
   * @returns {Array} Array of detected secrets
   */
  scanForSecrets(value) {
    /* istanbul ignore next */
    cov_259dpoh0rz().f[7]++;
    const startTime =
    /* istanbul ignore next */
    (cov_259dpoh0rz().s[26]++, Date.now());
    const detections =
    /* istanbul ignore next */
    (cov_259dpoh0rz().s[27]++, []);

    // Convert to string for scanning
    let scanValue =
    /* istanbul ignore next */
    (cov_259dpoh0rz().s[28]++, value);
    /* istanbul ignore next */
    cov_259dpoh0rz().s[29]++;
    if (typeof value === 'object') {
      /* istanbul ignore next */
      cov_259dpoh0rz().b[5][0]++;
      cov_259dpoh0rz().s[30]++;
      scanValue = JSON.stringify(value, null, 2);
    } else {
      /* istanbul ignore next */
      cov_259dpoh0rz().b[5][1]++;
      cov_259dpoh0rz().s[31]++;
      if (typeof value !== 'string') {
        /* istanbul ignore next */
        cov_259dpoh0rz().b[6][0]++;
        cov_259dpoh0rz().s[32]++;
        scanValue = String(value);
      } else
      /* istanbul ignore next */
      {
        cov_259dpoh0rz().b[6][1]++;
      }
    }

    // Check size limit
    /* istanbul ignore next */
    cov_259dpoh0rz().s[33]++;
    if (
    /* istanbul ignore next */
    (cov_259dpoh0rz().b[8][0]++, this.config.max_scan_size) &&
    /* istanbul ignore next */
    (cov_259dpoh0rz().b[8][1]++, scanValue.length > this.config.max_scan_size)) {
      /* istanbul ignore next */
      cov_259dpoh0rz().b[7][0]++;
      cov_259dpoh0rz().s[34]++;
      throw new Error(`Value exceeds maximum scan size of ${this.config.max_scan_size} bytes`);
    } else
    /* istanbul ignore next */
    {
      cov_259dpoh0rz().b[7][1]++;
    }

    // Skip if whitelisted
    cov_259dpoh0rz().s[35]++;
    if (this.isWhitelisted(scanValue)) {
      /* istanbul ignore next */
      cov_259dpoh0rz().b[9][0]++;
      cov_259dpoh0rz().s[36]++;
      return detections;
    } else
    /* istanbul ignore next */
    {
      cov_259dpoh0rz().b[9][1]++;
    }

    // Scan against all patterns
    cov_259dpoh0rz().s[37]++;
    for (const pattern of this.patterns) {
      const matches =
      /* istanbul ignore next */
      (cov_259dpoh0rz().s[38]++, scanValue.match(pattern.compiledRegex));
      /* istanbul ignore next */
      cov_259dpoh0rz().s[39]++;
      if (
      /* istanbul ignore next */
      (cov_259dpoh0rz().b[11][0]++, matches) &&
      /* istanbul ignore next */
      (cov_259dpoh0rz().b[11][1]++, matches.length > 0)) {
        /* istanbul ignore next */
        cov_259dpoh0rz().b[10][0]++;
        cov_259dpoh0rz().s[40]++;
        detections.push({
          name: pattern.name,
          severity: pattern.severity,
          description: pattern.description,
          recommendation: pattern.recommendation,
          matchCount: matches.length,
          // Only include first match snippet (redacted)
          sample: this.redactMatch(matches[0])
        });
      } else
      /* istanbul ignore next */
      {
        cov_259dpoh0rz().b[10][1]++;
      }
    }

    // Check performance timeout
    const scanTime =
    /* istanbul ignore next */
    (cov_259dpoh0rz().s[41]++, Date.now() - startTime);
    /* istanbul ignore next */
    cov_259dpoh0rz().s[42]++;
    if (
    /* istanbul ignore next */
    (cov_259dpoh0rz().b[13][0]++, this.config.performance_timeout_ms) &&
    /* istanbul ignore next */
    (cov_259dpoh0rz().b[13][1]++, scanTime > this.config.performance_timeout_ms)) {
      /* istanbul ignore next */
      cov_259dpoh0rz().b[12][0]++;
      cov_259dpoh0rz().s[43]++;
      console.warn(`Secret scan exceeded timeout: ${scanTime}ms > ${this.config.performance_timeout_ms}ms`);
    } else
    /* istanbul ignore next */
    {
      cov_259dpoh0rz().b[12][1]++;
    }
    cov_259dpoh0rz().s[44]++;
    return detections;
  }

  /**
   * Redact a matched secret for logging
   */
  redactMatch(match) {
    /* istanbul ignore next */
    cov_259dpoh0rz().f[8]++;
    cov_259dpoh0rz().s[45]++;
    if (match.length <= 8) {
      /* istanbul ignore next */
      cov_259dpoh0rz().b[14][0]++;
      cov_259dpoh0rz().s[46]++;
      return '***REDACTED***';
    } else
    /* istanbul ignore next */
    {
      cov_259dpoh0rz().b[14][1]++;
    }
    cov_259dpoh0rz().s[47]++;
    return match.substring(0, 4) + '***REDACTED***' + match.substring(match.length - 4);
  }

  /**
   * Validate that no secrets are present in value
   * @param {string} key - Memory key
   * @param {any} value - Value to validate
   * @throws {Error} If secrets are detected
   * @returns {boolean} True if validation passes
   */
  validateNoSecrets(key, value) {
    /* istanbul ignore next */
    cov_259dpoh0rz().f[9]++;
    const detections =
    /* istanbul ignore next */
    (cov_259dpoh0rz().s[48]++, this.scanForSecrets(value));
    /* istanbul ignore next */
    cov_259dpoh0rz().s[49]++;
    if (detections.length > 0) {
      /* istanbul ignore next */
      cov_259dpoh0rz().b[15][0]++;
      // Log violation
      const violation =
      /* istanbul ignore next */
      (cov_259dpoh0rz().s[50]++, {
        timestamp: new Date().toISOString(),
        key,
        detections: detections.map(d => {
          /* istanbul ignore next */
          cov_259dpoh0rz().f[10]++;
          cov_259dpoh0rz().s[51]++;
          return {
            name: d.name,
            severity: d.severity,
            description: d.description
          };
        }),
        blocked: this.config.block_on_detection
      });
      /* istanbul ignore next */
      cov_259dpoh0rz().s[52]++;
      this.violationLog.push(violation);

      // Audit if enabled
      /* istanbul ignore next */
      cov_259dpoh0rz().s[53]++;
      if (this.config.audit_violations) {
        /* istanbul ignore next */
        cov_259dpoh0rz().b[16][0]++;
        cov_259dpoh0rz().s[54]++;
        this.auditViolation(violation);
      } else
      /* istanbul ignore next */
      {
        cov_259dpoh0rz().b[16][1]++;
      }

      // Block if configured
      cov_259dpoh0rz().s[55]++;
      if (this.config.block_on_detection) {
        /* istanbul ignore next */
        cov_259dpoh0rz().b[17][0]++;
        const errorMessage =
        /* istanbul ignore next */
        (cov_259dpoh0rz().s[56]++, this.formatErrorMessage(detections));
        /* istanbul ignore next */
        cov_259dpoh0rz().s[57]++;
        throw new Error(errorMessage);
      } else
      /* istanbul ignore next */
      {
        cov_259dpoh0rz().b[17][1]++;
      }
      cov_259dpoh0rz().s[58]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_259dpoh0rz().b[15][1]++;
    }
    cov_259dpoh0rz().s[59]++;
    return true;
  }

  /**
   * Format user-friendly error message
   */
  formatErrorMessage(detections) {
    /* istanbul ignore next */
    cov_259dpoh0rz().f[11]++;
    const criticalCount =
    /* istanbul ignore next */
    (cov_259dpoh0rz().s[60]++, detections.filter(d => {
      /* istanbul ignore next */
      cov_259dpoh0rz().f[12]++;
      cov_259dpoh0rz().s[61]++;
      return d.severity === 'critical';
    }).length);
    const highCount =
    /* istanbul ignore next */
    (cov_259dpoh0rz().s[62]++, detections.filter(d => {
      /* istanbul ignore next */
      cov_259dpoh0rz().f[13]++;
      cov_259dpoh0rz().s[63]++;
      return d.severity === 'high';
    }).length);
    let message =
    /* istanbul ignore next */
    (cov_259dpoh0rz().s[64]++, ' SECRET DETECTED - Storage blocked for security!\n\n');
    /* istanbul ignore next */
    cov_259dpoh0rz().s[65]++;
    message += `Found ${detections.length} potential secret(s):\n`;
    /* istanbul ignore next */
    cov_259dpoh0rz().s[66]++;
    detections.forEach((detection, index) => {
      /* istanbul ignore next */
      cov_259dpoh0rz().f[14]++;
      cov_259dpoh0rz().s[67]++;
      message += `\n${index + 1}. ${detection.description}\n`;
      /* istanbul ignore next */
      cov_259dpoh0rz().s[68]++;
      message += `   Severity: ${detection.severity.toUpperCase()}\n`;
      /* istanbul ignore next */
      cov_259dpoh0rz().s[69]++;
      message += `    ${detection.recommendation}\n`;
    });
    /* istanbul ignore next */
    cov_259dpoh0rz().s[70]++;
    message += '\n Best Practice: Store secrets in environment variables or secure vaults.\n';
    /* istanbul ignore next */
    cov_259dpoh0rz().s[71]++;
    message += '   Never commit secrets to version control or memory storage.\n';
    /* istanbul ignore next */
    cov_259dpoh0rz().s[72]++;
    return message;
  }

  /**
   * Audit violation to log file
   */
  auditViolation(violation) {
    /* istanbul ignore next */
    cov_259dpoh0rz().f[15]++;
    cov_259dpoh0rz().s[73]++;
    try {
      const logDir =
      /* istanbul ignore next */
      (cov_259dpoh0rz().s[74]++, path.join(__dirname, '../../logs/12fa'));
      const logFile =
      /* istanbul ignore next */
      (cov_259dpoh0rz().s[75]++, path.join(logDir, 'secrets-violations.log'));

      // Create log directory if it doesn't exist
      /* istanbul ignore next */
      cov_259dpoh0rz().s[76]++;
      if (!fs.existsSync(logDir)) {
        /* istanbul ignore next */
        cov_259dpoh0rz().b[18][0]++;
        cov_259dpoh0rz().s[77]++;
        fs.mkdirSync(logDir, {
          recursive: true
        });
      } else
      /* istanbul ignore next */
      {
        cov_259dpoh0rz().b[18][1]++;
      }

      // Append to log file
      const logEntry =
      /* istanbul ignore next */
      (cov_259dpoh0rz().s[78]++, JSON.stringify(violation) + '\n');
      /* istanbul ignore next */
      cov_259dpoh0rz().s[79]++;
      fs.appendFileSync(logFile, logEntry, 'utf8');
    } catch (error) {
      /* istanbul ignore next */
      cov_259dpoh0rz().s[80]++;
      console.error('Failed to audit violation:', error.message);
    }
  }

  /**
   * Get violation statistics
   */
  getViolationStats() {
    /* istanbul ignore next */
    cov_259dpoh0rz().f[16]++;
    const stats =
    /* istanbul ignore next */
    (cov_259dpoh0rz().s[81]++, {
      total: this.violationLog.length,
      bySeverity: {},
      byPattern: {},
      recent: this.violationLog.slice(-10)
    });
    /* istanbul ignore next */
    cov_259dpoh0rz().s[82]++;
    this.violationLog.forEach(violation => {
      /* istanbul ignore next */
      cov_259dpoh0rz().f[17]++;
      cov_259dpoh0rz().s[83]++;
      violation.detections.forEach(detection => {
        /* istanbul ignore next */
        cov_259dpoh0rz().f[18]++;
        cov_259dpoh0rz().s[84]++;
        // Count by severity
        stats.bySeverity[detection.severity] = (
        /* istanbul ignore next */
        (cov_259dpoh0rz().b[19][0]++, stats.bySeverity[detection.severity]) ||
        /* istanbul ignore next */
        (cov_259dpoh0rz().b[19][1]++, 0)) + 1;

        // Count by pattern name
        /* istanbul ignore next */
        cov_259dpoh0rz().s[85]++;
        stats.byPattern[detection.name] = (
        /* istanbul ignore next */
        (cov_259dpoh0rz().b[20][0]++, stats.byPattern[detection.name]) ||
