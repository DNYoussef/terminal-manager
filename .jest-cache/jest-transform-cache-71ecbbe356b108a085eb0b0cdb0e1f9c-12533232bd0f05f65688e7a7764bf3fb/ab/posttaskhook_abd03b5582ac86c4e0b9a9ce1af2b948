          span_id: span.spanId,
          agent: taskResult.agentType,
          reason: deductResult.reason
        });
      }
    } else
    /* istanbul ignore next */
    {
      cov_3md98hu4y().b[22][1]++;
    }
    cov_3md98hu4y().s[67]++;
    otelAdapter.endSpan(span);
    /* istanbul ignore next */
    cov_3md98hu4y().s[68]++;
    return {
      success: true,
      taskResult,
      hookDuration,
      trace_id: actualCorrelationId,
      span_id: span.spanId,
      timestamp: new Date().toISOString()
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_3md98hu4y().s[69]++;
    logger.error('Post-task hook execution failed', {
      trace_id: actualCorrelationId,
      span_id: span.spanId,
      error: error.message,
      status: 'error'
    });
    /* istanbul ignore next */
    cov_3md98hu4y().s[70]++;
    span.recordException(error);
    /* istanbul ignore next */
    cov_3md98hu4y().s[71]++;
    otelAdapter.endSpan(span);
    /* istanbul ignore next */
    cov_3md98hu4y().s[72]++;
    return {
      success: false,
      error: error.message,
      stack: error.stack,
      trace_id: actualCorrelationId,
      span_id: span.spanId,
      timestamp: new Date().toISOString()
    };
  }
}

/**
 * Update task metrics in persistent storage
 */
async function updateTaskMetrics(taskResult, correlationId, spanId) {
  /* istanbul ignore next */
  cov_3md98hu4y().f[1]++;
  cov_3md98hu4y().s[73]++;
  try {
    const metricsPath =
    /* istanbul ignore next */
    (cov_3md98hu4y().s[74]++, path.join(__dirname, '../../logs/12fa/task-metrics.json'));
    let metrics =
    /* istanbul ignore next */
    (cov_3md98hu4y().s[75]++, {
      totalTasks: 0,
      successfulTasks: 0,
      failedTasks: 0,
      totalDuration: 0,
      averageDuration: 0,
      lastTask: null,
      tasks: {}
    });

    // Load existing metrics
    /* istanbul ignore next */
    cov_3md98hu4y().s[76]++;
    if (fs.existsSync(metricsPath)) {
      /* istanbul ignore next */
      cov_3md98hu4y().b[28][0]++;
      const data =
      /* istanbul ignore next */
      (cov_3md98hu4y().s[77]++, fs.readFileSync(metricsPath, 'utf8'));
      /* istanbul ignore next */
      cov_3md98hu4y().s[78]++;
      metrics = JSON.parse(data);
    } else
    /* istanbul ignore next */
    {
      cov_3md98hu4y().b[28][1]++;
    }

    // Update metrics
    cov_3md98hu4y().s[79]++;
    metrics.totalTasks++;
    /* istanbul ignore next */
    cov_3md98hu4y().s[80]++;
    if (
    /* istanbul ignore next */
    (cov_3md98hu4y().b[30][0]++, taskResult.status === 'completed') ||
    /* istanbul ignore next */
    (cov_3md98hu4y().b[30][1]++, taskResult.status === 'success')) {
      /* istanbul ignore next */
      cov_3md98hu4y().b[29][0]++;
      cov_3md98hu4y().s[81]++;
      metrics.successfulTasks++;
    } else {
      /* istanbul ignore next */
      cov_3md98hu4y().b[29][1]++;
      cov_3md98hu4y().s[82]++;
      if (taskResult.status === 'failed') {
        /* istanbul ignore next */
        cov_3md98hu4y().b[31][0]++;
        cov_3md98hu4y().s[83]++;
        metrics.failedTasks++;
      } else
      /* istanbul ignore next */
      {
        cov_3md98hu4y().b[31][1]++;
      }
    }
    /* istanbul ignore next */
    cov_3md98hu4y().s[84]++;
    if (taskResult.duration) {
      /* istanbul ignore next */
      cov_3md98hu4y().b[32][0]++;
      cov_3md98hu4y().s[85]++;
      metrics.totalDuration += taskResult.duration;
      /* istanbul ignore next */
      cov_3md98hu4y().s[86]++;
      metrics.averageDuration = metrics.totalDuration / metrics.totalTasks;
    } else
    /* istanbul ignore next */
    {
      cov_3md98hu4y().b[32][1]++;
    }
    cov_3md98hu4y().s[87]++;
    metrics.lastTask = new Date().toISOString();
    /* istanbul ignore next */
    cov_3md98hu4y().s[88]++;
    metrics.tasks[taskResult.taskId] = {
      ...taskResult,
      trace_id: correlationId,
      span_id: spanId
    };

    // Save metrics
    const metricsDir =
    /* istanbul ignore next */
    (cov_3md98hu4y().s[89]++, path.dirname(metricsPath));
    /* istanbul ignore next */
    cov_3md98hu4y().s[90]++;
    if (!fs.existsSync(metricsDir)) {
      /* istanbul ignore next */
      cov_3md98hu4y().b[33][0]++;
      cov_3md98hu4y().s[91]++;
      fs.mkdirSync(metricsDir, {
        recursive: true
      });
    } else
    /* istanbul ignore next */
    {
      cov_3md98hu4y().b[33][1]++;
    }
    cov_3md98hu4y().s[92]++;
    fs.writeFileSync(metricsPath, JSON.stringify(metrics, null, 2), 'utf8');
    /* istanbul ignore next */
    cov_3md98hu4y().s[93]++;
    logger.debug('Task metrics updated', {
      trace_id: correlationId,
      span_id: spanId,
      total_tasks: metrics.totalTasks,
      success_rate: (metrics.successfulTasks / metrics.totalTasks * 100).toFixed(2) + '%'
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_3md98hu4y().s[94]++;
    logger.error('Failed to update task metrics', {
      trace_id: correlationId,
      span_id: spanId,
      error: error.message
    });
  }
}

/**
 * Get task metrics
 */
function getTaskMetrics() {
  /* istanbul ignore next */
  cov_3md98hu4y().f[2]++;
  cov_3md98hu4y().s[95]++;
  try {
    const metricsPath =
    /* istanbul ignore next */
    (cov_3md98hu4y().s[96]++, path.join(__dirname, '../../logs/12fa/task-metrics.json'));
    /* istanbul ignore next */
    cov_3md98hu4y().s[97]++;
    if (fs.existsSync(metricsPath)) {
      /* istanbul ignore next */
      cov_3md98hu4y().b[34][0]++;
      const data =
      /* istanbul ignore next */
      (cov_3md98hu4y().s[98]++, fs.readFileSync(metricsPath, 'utf8'));
      /* istanbul ignore next */
      cov_3md98hu4y().s[99]++;
      return JSON.parse(data);
    } else
    /* istanbul ignore next */
    {
      cov_3md98hu4y().b[34][1]++;
