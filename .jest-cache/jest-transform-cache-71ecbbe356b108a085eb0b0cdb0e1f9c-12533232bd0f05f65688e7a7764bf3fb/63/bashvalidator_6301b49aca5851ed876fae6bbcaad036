      /* istanbul ignore next */
      cov_1y9op1ic32().b[29][0]++;
      const allowedCheck =
      /* istanbul ignore next */
      (cov_1y9op1ic32().s[70]++, this.isCommandAllowed(command));
      /* istanbul ignore next */
      cov_1y9op1ic32().s[71]++;
      if (!allowedCheck.allowed) {
        /* istanbul ignore next */
        cov_1y9op1ic32().b[31][0]++;
        cov_1y9op1ic32().s[72]++;
        return this.createViolation(command, 'NOT_ALLOWED', {
          reason: allowedCheck.reason
        });
      } else
      /* istanbul ignore next */
      {
        cov_1y9op1ic32().b[31][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_1y9op1ic32().b[29][1]++;
    }

    // Command is valid
    const validationTime =
    /* istanbul ignore next */
    (cov_1y9op1ic32().s[73]++, Date.now() - validationStart);
    /* istanbul ignore next */
    cov_1y9op1ic32().s[74]++;
    return {
      valid: true,
      command,
      policy: this.activePolicy,
      agentType: this.agentType,
      validationTime,
      timestamp: new Date().toISOString()
    };
  }

  /**
   * Create violation record
   */
  createViolation(command, violationType, details =
  /* istanbul ignore next */
  (cov_1y9op1ic32().b[32][0]++, {})) {
    /* istanbul ignore next */
    cov_1y9op1ic32().f[8]++;
    const violation =
    /* istanbul ignore next */
    (cov_1y9op1ic32().s[75]++, {
      valid: false,
      command,
      violationType,
      policy: this.activePolicy,
      agentType: this.agentType,
      details,
      timestamp: new Date().toISOString(),
      message: this.getViolationMessage(violationType, command, details)
    });
    /* istanbul ignore next */
    cov_1y9op1ic32().s[76]++;
    this.violations.push(violation);
    /* istanbul ignore next */
    cov_1y9op1ic32().s[77]++;
    this.logViolation(violation);
    /* istanbul ignore next */
    cov_1y9op1ic32().s[78]++;
    return violation;
  }

  /**
   * Get human-readable violation message
   */
  getViolationMessage(type, command, details) {
    /* istanbul ignore next */
    cov_1y9op1ic32().f[9]++;
    const {
      base
    } =
    /* istanbul ignore next */
    (cov_1y9op1ic32().s[79]++, this.extractBaseCommand(command));
    /* istanbul ignore next */
    cov_1y9op1ic32().s[80]++;
    switch (type) {
      case 'BLOCKED_COMMAND':
        /* istanbul ignore next */
        cov_1y9op1ic32().b[33][0]++;
        cov_1y9op1ic32().s[81]++;
        return ` BLOCKED: Command "${base}" is explicitly blocked.\n` + `Reason: ${details.reason}\n` + `Policy: ${this.activePolicy}\n` + `This command poses security risks and cannot be executed.`;
      case 'NOT_ALLOWED':
        /* istanbul ignore next */
        cov_1y9op1ic32().b[33][1]++;
        cov_1y9op1ic32().s[82]++;
        return ` NOT ALLOWED: Command "${base}" is not in the allowlist.\n` + `Reason: ${details.reason}\n` + `Policy: ${this.activePolicy}\n` + `Only explicitly allowed commands can be executed in ${this.activePolicy} mode.`;
      case 'COMMAND_TOO_LONG':
        /* istanbul ignore next */
        cov_1y9op1ic32().b[33][2]++;
        cov_1y9op1ic32().s[83]++;
        return ` COMMAND TOO LONG: Command exceeds maximum length.\n` + `Length: ${details.length} characters (max: ${details.maxLength})\n` + `This may indicate a buffer overflow attack.`;
      default:
        /* istanbul ignore next */
        cov_1y9op1ic32().b[33][3]++;
        cov_1y9op1ic32().s[84]++;
        return ` VALIDATION FAILED: ${type}`;
    }
  }

  /**
   * Log violation to file
   */
  logViolation(violation) {
    /* istanbul ignore next */
    cov_1y9op1ic32().f[10]++;
    cov_1y9op1ic32().s[85]++;
    if (!this.policy?.monitoring?.log_violations) {
      /* istanbul ignore next */
      cov_1y9op1ic32().b[34][0]++;
      cov_1y9op1ic32().s[86]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_1y9op1ic32().b[34][1]++;
    }
    const logFile =
    /* istanbul ignore next */
    (cov_1y9op1ic32().s[87]++,
    /* istanbul ignore next */
    (cov_1y9op1ic32().b[35][0]++, this.policy.monitoring.log_file) ||
    /* istanbul ignore next */
    (cov_1y9op1ic32().b[35][1]++, 'logs/bash-violations.log'));
    const logDir =
    /* istanbul ignore next */
    (cov_1y9op1ic32().s[88]++, path.dirname(logFile));

    // Ensure log directory exists
    /* istanbul ignore next */
    cov_1y9op1ic32().s[89]++;
    if (!fs.existsSync(logDir)) {
      /* istanbul ignore next */
      cov_1y9op1ic32().b[36][0]++;
      cov_1y9op1ic32().s[90]++;
      fs.mkdirSync(logDir, {
        recursive: true
      });
    } else
    /* istanbul ignore next */
    {
      cov_1y9op1ic32().b[36][1]++;
    }
    const logEntry =
    /* istanbul ignore next */
    (cov_1y9op1ic32().s[91]++, {
      ...violation,
      sessionId:
      /* istanbul ignore next */
      (cov_1y9op1ic32().b[37][0]++, process.env.CLAUDE_FLOW_SESSION_ID) ||
      /* istanbul ignore next */
      (cov_1y9op1ic32().b[37][1]++, 'unknown'),
      pid: process.pid
    });
    /* istanbul ignore next */
    cov_1y9op1ic32().s[92]++;
    fs.appendFileSync(logFile, JSON.stringify(logEntry) + '\n', 'utf8');
  }

  /**
   * Get violation statistics
   */
  getViolationStats() {
    /* istanbul ignore next */
    cov_1y9op1ic32().f[11]++;
    const now =
    /* istanbul ignore next */
    (cov_1y9op1ic32().s[93]++, Date.now());
    const oneMinuteAgo =
    /* istanbul ignore next */
    (cov_1y9op1ic32().s[94]++, now - 60000);
    const recentViolations =
    /* istanbul ignore next */
    (cov_1y9op1ic32().s[95]++, this.violations.filter(v => {
      /* istanbul ignore next */
      cov_1y9op1ic32().f[12]++;
      cov_1y9op1ic32().s[96]++;
      return new Date(v.timestamp).getTime() > oneMinuteAgo;
    }));
    /* istanbul ignore next */
    cov_1y9op1ic32().s[97]++;
    return {
      total: this.violations.length,
      lastMinute: recentViolations.length,
      byType: this.violations.reduce((acc, v) => {
        /* istanbul ignore next */
        cov_1y9op1ic32().f[13]++;
        cov_1y9op1ic32().s[98]++;
        acc[v.violationType] = (
        /* istanbul ignore next */
        (cov_1y9op1ic32().b[38][0]++, acc[v.violationType]) ||
        /* istanbul ignore next */
        (cov_1y9op1ic32().b[38][1]++, 0)) + 1;
        /* istanbul ignore next */
        cov_1y9op1ic32().s[99]++;
        return acc;
      }, {}),
      uptime: now - this.startTime
    };
  }

  /**
   * Check if violation rate exceeds threshold
   */
  isViolationRateHigh() {
    /* istanbul ignore next */
    cov_1y9op1ic32().f[14]++;
    const stats =
    /* istanbul ignore next */
    (cov_1y9op1ic32().s[100]++, this.getViolationStats());
    const threshold =
    /* istanbul ignore next */
    (cov_1y9op1ic32().s[101]++,
    /* istanbul ignore next */
    (cov_1y9op1ic32().b[39][0]++, this.policy?.monitoring?.alert_threshold) ||
    /* istanbul ignore next */
    (cov_1y9op1ic32().b[39][1]++, 5));
    /* istanbul ignore next */
    cov_1y9op1ic32().s[102]++;
    return stats.lastMinute >= threshold;
  }

  /**
   * Export metrics to file
   */
  exportMetrics() {
    /* istanbul ignore next */
    cov_1y9op1ic32().f[15]++;
    cov_1y9op1ic32().s[103]++;
    if (!this.policy?.monitoring?.export_metrics) {
      /* istanbul ignore next */
      cov_1y9op1ic32().b[40][0]++;
      cov_1y9op1ic32().s[104]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_1y9op1ic32().b[40][1]++;
    }
    const metricsFile =
    /* istanbul ignore next */
    (cov_1y9op1ic32().s[105]++,
    /* istanbul ignore next */
    (cov_1y9op1ic32().b[41][0]++, this.policy.monitoring.metrics_file) ||
    /* istanbul ignore next */
    (cov_1y9op1ic32().b[41][1]++, 'metrics/bash-policy-metrics.json'));
    const metricsDir =
    /* istanbul ignore next */
    (cov_1y9op1ic32().s[106]++, path.dirname(metricsFile));
    /* istanbul ignore next */
    cov_1y9op1ic32().s[107]++;
    if (!fs.existsSync(metricsDir)) {
      /* istanbul ignore next */
      cov_1y9op1ic32().b[42][0]++;
      cov_1y9op1ic32().s[108]++;
      fs.mkdirSync(metricsDir, {
        recursive: true
      });
    } else
    /* istanbul ignore next */
    {
      cov_1y9op1ic32().b[42][1]++;
    }
    const metrics =
    /* istanbul ignore next */
    (cov_1y9op1ic32().s[109]++, {
      timestamp: new Date().toISOString(),
      policy: this.activePolicy,
      agentType: this.agentType,
      stats: this.getViolationStats(),
      violations: this.violations,
      version: this.policy.version
    });
    /* istanbul ignore next */
    cov_1y9op1ic32().s[110]++;
    fs.writeFileSync(metricsFile, JSON.stringify(metrics, null, 2), 'utf8');
  }
}

